{
    "name": "zgabievi/laravel-promocodes",
    "description": "Generate promotional codes for Laravel",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/zgabievi/laravel-promocodes",
    "keywords": [
        "laravel",
        "promocode",
        "coupon"
    ],
    "authors": [
        {
            "name": "Zura Gabievi",
            "email": "zura.gabievi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zorb\\Promocodes\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zorb\\Promocodes\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "predis/predis": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zorb\\Promocodes\\PromocodesServiceProvider"
            ],
            "aliases": {
                "Promocodes": "Zorb\\Promocodes\\Facades\\Promocodes"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
